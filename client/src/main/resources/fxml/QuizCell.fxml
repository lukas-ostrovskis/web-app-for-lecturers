<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListCell?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="120.0" prefWidth="900.0"
         styleClass="listCell" stylesheets="@../css/app.css"
         textFill="#51a3cf"
         type="javafx.scene.control.ListCell"
         xmlns="http://javafx.com/javafx/11.0.1">
    <graphic>
        <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
              minWidth="-Infinity" prefHeight="150.0" prefWidth="1000.0">
            <children>
                <VBox maxWidth="1.7976931348623157E308" prefHeight="131.0"
                      prefWidth="468.0">
                    <children>
                        <AnchorPane prefHeight="84.0">
                            <children>
                                <TextArea fx:id="content" layoutX="14.0"
                                          layoutY="-13.0" prefHeight="124.0"
                                          prefWidth="446.0"/>
                            </children>
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                            <padding>
                                <Insets top="20.0"/>
                            </padding>
                        </AnchorPane>
                    </children>
                </VBox>
                <VBox prefHeight="101.0" prefWidth="213.0">
                    <children>
                        <Button fx:id="toggleStatus" minWidth="137.0"
                                mnemonicParsing="false"
                                onAction="#toggleButtonPressed"
                                prefHeight="34.0" prefWidth="213.0"
                                text="Open/Close"/>
                        <Button fx:id="answerDistributionReady" layoutX="10.0"
                                layoutY="10.0" minWidth="137.0"
                                mnemonicParsing="false"
                                onAction="#toggleAnswerDistributionReadyPressed"
                                prefHeight="34.0" prefWidth="216.0"
                                text="Answer distribution"/>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="25.0" top="25.0"/>
            </padding>
        </HBox>
    </graphic>
</fx:root>
